  atk-runtime-1.9.1-1
  cygwin-1.5.18-1
  expat-1.95.8-1
  glib2-runtime-2.6.6-1
  libXft-2.1.6-1
  libfontconfig1-2.2.2-1
  libfreetype26-2.1.5-1
  libiconv2-1.9.2-1
  libintl3-0.14.1-1
  libjpeg62-6b-11
  libpng12-1.2.8-2
  libtiff5-3.7.3-1
  pango-runtime-1.8.2-1
  xorg-x11-bin-dlls-6.8.2.0-1
  zlib-1.2.2-2
