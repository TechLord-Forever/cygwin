prefix=/usr
exec_prefix=/usr
libdir=/usr/lib
includedir=/usr/include

Name: Pango Xft
Description: Xft font support for Pango
Version: 1.8.1
Requires: pango
Libs: -L${libdir} -lpangoxft-1.0 -L/usr/X11R6/lib -lXft -lfreetype -lz -lXrender -lXext -lX11 -lfontconfig  
Cflags: -I${includedir}/pango-1.0 -I/usr/X11R6/include -I/usr/include/freetype2  
