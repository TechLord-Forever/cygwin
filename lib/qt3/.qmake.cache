CONFIG +=  nocrosscompiler minimal-config small-config medium-config large-config full-config styles tools kernel widgets dialogs iconview workspace network canvas table xml opengl sql release dll thread largefile stl system-mng system-jpeg system-png gif system-zlib bigcodecs x11sm xshape xcursor xrandr xrender xftfreetype tablet xkb dylib create_prl
QMAKESPEC = cygwin-g++
QT_SOURCE_TREE = /usr/lib/qt3
QT_BUILD_TREE = /usr/lib/qt3
QMAKE_ABSOLUTE_SOURCE_ROOT = $$QT_SOURCE_TREE
QT_PRODUCT=qt-free
styles += windows motif mac platinum sgi cde motifplus
kbd-drivers += tty
mouse-drivers += pc
styles += cde
styles += compact
styles += motif
styles += motifplus
styles += platinum
styles += sgi
styles += windows
sql-drivers -= ibase
sql-plugins -= ibase
sql-drivers -= mysql
sql-plugins -= mysql
sql-drivers -= odbc
sql-plugins -= odbc
sql-plugins += psql
sql-plugins += sqlite
QMAKE_MOC=/usr/lib/qt3/bin/moc
QMAKE_UIC=/usr/lib/qt3/bin/uic -L $$QT_BUILD_TREE/plugins
QMAKE_QMAKE=/usr/lib/qt3/bin/qmake
QMAKE_MOC_SRC=/usr/lib/qt3/src/moc
INCLUDEPATH+=/usr/include/freetype2
QMAKE_LIBS_X11=-lXft -lfreetype -lfontconfig $$QMAKE_LIBS_X11
QMAKE_INCDIR_QT=/usr/include/qt3
QMAKE_LIBDIR_QT=/usr/lib/qt3/lib
QMAKE_RTLDIR_QT=/usr/lib/qt3/lib
OBJECTS_DIR=.obj/release-shared-mt
MOC_DIR=.moc/release-shared-mt
QMAKE_LIBS_QT+=-lz
QMAKE_LIBS_QT_THREAD+=-lz
QMAKE_LIBS_X11 = -lXcursor $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXrandr $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXrender $$QMAKE_LIBS_X11
QMAKE_LIBS_X11 = -lXi $$QMAKE_LIBS_X11
LIBS+= -lresolv
QMAKE_CFLAGS+= -I/usr/include/postgresql/server -I/usr/include/postgresql/pgsql -I/usr/include/postgresql/pgsql/server
QMAKE_CXXFLAGS+= -I/usr/include/postgresql/server -I/usr/include/postgresql/pgsql -I/usr/include/postgresql/pgsql/server
QT_INSTALL_PREFIX=/usr/lib/qt3
docs.path=/usr/share/qt3/doc
headers.path=/usr/include/qt3
plugins.path=/usr/lib/qt3/plugins
libs.path=/usr/lib/qt3/lib
bins.path=/usr/lib/qt3/bin
data.path=/usr/share/qt3
translations.path=/usr/share/qt3/translations
QMAKE_CFLAGS+=-fno-exceptions
QMAKE_CXXFLAGS+=-fno-exceptions
QMAKE_LFLAGS+=-fno-exceptions
